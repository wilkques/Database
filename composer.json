{
    "name": "wilkques/database",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": ">=5.4",
        "wilkques/php-helper": "^5.13.0"
    },
    "require-dev": {
        "phpunit/phpunit": "*",
        "mockery/mockery": "*",
        "vlucas/phpdotenv": "*",
        "wilkques/Container": "*"
    },
    "autoload": {
        "psr-4": {
            "Wilkques\\Database\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Wilkques\\Database\\Tests\\": "tests/"
        }
    },
    "suggest": {
        "wilkques/container": "PHP with Container",
        "ext-pdo": "Required to use the PDO"
    }
}
